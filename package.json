{
  "name": "trc20-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
	"build": "nest build",
	"dev": "NODE_ENV=development nest start",
	"dev:watch": "NODE_ENV=development nest start --watch",
	"start": "node dist/main",
	"typeorm": "ts-node -r tsconfig-paths/register  ./node_modules/.bin/typeorm",
	"migration:create": "npm run typeorm -- migration:create",
	"migration:generate": "npm run schema:drop && npm run migration:run && npm run typeorm -- migration:generate --dataSource src/db/data-source.ts",
	"migration:run": "npm run typeorm -- migration:run --dataSource src/db/data-source.ts",
	"migration:revert": "npm run typeorm -- migration:revert --dataSource src/db/data-source.ts",
	"migration:show": "npm run typeorm -- migration:show --dataSource src/db/data-source.ts",
	"schema:drop": "npm run typeorm -- schema:drop --dataSource src/db/data-source.ts",
	"cache:clear": "npm run typeorm -- -d src/db/data-source.ts"
  },
  "dependencies": {
	"@nestjs/common": "^9.0.0",
	"@nestjs/core": "^9.0.0",
	"@nestjs/platform-express": "^9.0.0",
	"@nestjs/typeorm": "^9.0.1",
	"axios": "^1.4.0",
	"class-transformer": "^0.5.1",
	"class-validator": "^0.14.0",
	"date-fns": "^2.30.0",
	"dotenv": "^16.1.4",
	"ethers": "^5.7.2",
	"jest-config": "^29.5.0",
	"js-base64": "^3.7.5",
	"lodash": "^4.17.21",
	"nestjs-pino": "^3.2.0",
	"pg": "^8.11.0",
	"pino-http": "^8.3.3",
	"reflect-metadata": "^0.1.13",
	"rxjs": "^7.8.1",
	"tronweb": "^5.2.0",
	"typeorm": "^0.3.16",
	"web3": "^4.0.1",
	"@nestjs/cli": "^9.0.0",
	"@nestjs/schematics": "^9.0.0"
  },
  "devDependencies": {
	"@nestjs/testing": "^9.0.0",
	"@types/express": "^4.17.13",
	"@types/jest": "^29.2.4",
	"@types/lodash": "^4.14.195",
	"@types/node": "18.11.18",
	"@types/supertest": "^2.0.11",
	"@typescript-eslint/eslint-plugin": "^5.0.0",
	"@typescript-eslint/parser": "^5.0.0",
	"eslint": "^8.0.1",
	"eslint-config-prettier": "^8.3.0",
	"eslint-plugin-prettier": "^4.0.0",
	"jest": "^29.3.1",
	"source-map-support": "^0.5.20",
	"ts-jest": "^29.0.3",
	"ts-loader": "^9.2.3",
	"ts-node": "^10.0.0",
	"tsconfig-paths": "4.1.1",
	"typescript": "^4.7.4"
  }
}
